{
  "name": "nexa-saas-template",
  "version": "1.0.0",
  "description": "Modern SaaS Startup Template built with Tailwind CSS",
  "scripts": {
    "dev": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build": "tailwindcss -i ./src/input.css -o ./dist/output.css",
    "prod": "tailwindcss -i ./src/input.css -o ./dist/output.min.css --minify"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "keywords": ["saas", "template", "tailwind", "startup"],
  "author": "Cat Coders",
  "license": "MIT"
}